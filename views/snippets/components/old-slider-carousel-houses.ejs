<style>
    /* House Image Carousel with Information */

.carousel-wrapper *, .carousel-wrapper *::before, .carousel-wrapper *::after  {
margin: 0;
padding: 0;
box-sizing: border-box;
}

body.no-scroll {
overflow: hidden;
}

.houses-title {
font-size: 3rem;
font-weight: bold;
text-align: center;
color: rgb(var(--primary-color));
}

.carousel-wrapper {
height: auto;
width: 100vw;
position: relative;
display: flex;
justify-content: center;
margin-bottom: 2rem;
z-index: 0;
}

.carousel-container {
position: relative;
height: 450px;
width: 95vw;
overflow: hidden;
z-index: 0;
}

.carousel-tiles {
position: absolute;
top: 0;
height: 100%;
transform: translateX(0);
display: flex;
justify-content: center;
align-items: center;
transition: transform 1s ease-in-out;
z-index: 0;
}

.carousel-tile {
width: calc((95vw / 3) - 8rem);
height: 75%;
margin: 0 4rem;
border-radius: 1rem;
position: relative;
transition: transform 1.5s cubic-bezier(0.165, 0.84, 0.44, 1);
}

.carousel-tile::before {
content: "";
position: absolute;
width: 100%;
height: 100%;
border-radius: 1rem;
box-shadow: 0 0 2px rgba(0, 0, 0, 0.4);
transition: opacity 1.5s cubic-bezier(0.165, 0.84, 0.44, 1);
opacity: 1;
}

.carousel-tile::after {
content: "";
position: absolute;
width: 100%;
height: 100%;
border-radius: 1rem;
box-shadow: 0 0 15px rgba(0, 0, 0, 0.25);
transition: opacity 1.5s cubic-bezier(0.165, 0.84, 0.44, 1);
opacity: 0;
z-index: -1;
}

.carousel-tile.middle::before {
opacity: 0;
}

.carousel-tile.middle {
transform: scale(1.2);
}

.carousel-tile.middle::after {
opacity: 1;
}

.carousel-tile .image-wrapper {
position: absolute;
width: 100%;
height: 100%;
border-radius: 1rem;
}

.image-wrapper.manor-I {
background: url("/images/assisted-living-houses/golden-gate-manor-i-house-outside_600.jpg") no-repeat center/cover;
}

.image-wrapper.manor-II {
background: url("/images/assisted-living-houses/golden-gate-manor-ii-house-outside_600.jpg") no-repeat center/cover;
}

.image-wrapper.view {
background: url("/images/assisted-living-houses/golden-view-house-outside_600.jpg") no-repeat center/cover;
}

.image-wrapper.valley {
background: url("/images/assisted-living-houses/golden-valley-house-outside_600.jpg") no-repeat center/cover;
}

.horizon-image1 {
top: 0;
left: 0;
background: url("/images/assisted-living-houses/side-horizon_600.jpg") no-repeat center/cover;
}

.horizon-image2 {
top: 0;
right: 0;
background: url("/images/assisted-living-houses/backyard-seating-horizon_600.jpg") no-repeat center/cover;
}

.horizon-image3 {
bottom: 0;
left: 0;
background: url("/images/assisted-living-houses/front-horizon-edited_600.jpg") no-repeat center/cover;
}

.horizon-image4 {
bottom: 0;
right: 0;
background: url("/images/assisted-living-houses/Backyard-horizon_600.jpg") no-repeat center/cover;
}

.image-wrapper.horizon {
background: black;
overflow: hidden;
}

.quad-image-horizon {
width: 50%;
height: 50%;
position: absolute;
}

.carousel-tile .tile-title {
position: absolute;
z-index: 1;
top: 0;
left: 0;
border-top-left-radius: 1rem;
border-top-right-radius: 1rem;
background-color: rgba(0, 0, 0, 0.5);
width: 100%;
height: 4rem;
display: flex;
justify-content: center;
align-items: center;
color: white;
text-align: center;
}

.carousel-tile .tile-description {
position: absolute;
width: 100%;
height: 4rem;
background-color: rgba(0, 0, 0, 0.5);
display: flex;
justify-content: center;
align-items: center;
color: white;
top: 4rem;
text-align: center;
padding-bottom: 1rem;
transform: scale(0);
opacity: 0;
transition: transform 0.5s ease-in-out, opacity 0.5s ease-in-out;
z-index: 0;
}

.carousel-tile .image-wrapper:hover ~ .tile-description,
.info-btns-wrapper:hover ~ .tile-description,
.tile-description:hover {
transform: scale(1);
opacity: 1;
}

.info-btns-wrapper {
position: absolute;
bottom: 0;
padding: 0 1rem;
padding-bottom: 1rem;
width: 100%;
display: flex;
justify-content: space-around;
align-items: center;
gap: 2rem;
}

.info-btn {
border: none;
background: none;
padding: 1rem 0;
font-size: 1rem;
background-color: rgba(0, 0, 0, 0.5);
cursor: pointer;
color: rgba(255, 255, 255, 0.5);
border-radius: 1rem;
font-family: inherit;
text-align: center;
white-space: nowrap;
flex-grow: 1;
flex-shrink: 1;
}

a.info-btn {
text-decoration: none;
}

.info-btn:hover {
color: white;
background-color: black;
}

a.info-btn:hover {
    color: white;
}

.btn-wrapper {
top: 0;
left: 0;
position: absolute;
width: 100vw;
height: 100%;
display: flex;
align-items: center;
justify-content: space-between;
padding: 0 3rem;
}

.scroll-btn {
width: 3rem;
height: 20%;
background: rgba(50, 50, 50, 1);
border: none;
border-radius: 5px;
color: black;
cursor: pointer;
z-index: 2;
}

.scroll-btn:hover {
color: rgba(255, 255, 255, 0.7);
background: rgba(50, 50, 50, 0.7);
}

@media (min-width: 2000px) {
.carousel-tile {
    width: calc((95vw / 4) - 4rem);
    transition: none;
    margin: 0 2rem;
}
}


@media (max-width: 1050px) {
.carousel-tile {
    width: calc((95vw / 2) - 4rem);
    transition: none;
    margin: 0 2rem;
}

}

@media (max-width: 700px) {
.carousel-tile {
    width: calc((95vw - 4rem));
    transition: none;
    margin: 0 2rem;
}
}

/* More photos slider */


.picture-slider {
height: 100vh;
width: 100vw;
background-color: rgba(0, 0, 0, 0.5);
position: fixed;
top: 0;
display: flex;
justify-content: center;
z-index: 10000;
}

.picture-slides {
height: 100%;
width: 80%;
background-color: white;
position: relative;
}

.slide {
position: absolute;
top: 0;
left: 0;
height: 100%;
width: 100%;
list-style: none;
opacity: 0;
transition: opacity 0.5s ease-in-out;
}

.slide.active {
opacity: 1;
}

.full-image {
height: 100%;
width: 100%;
}

