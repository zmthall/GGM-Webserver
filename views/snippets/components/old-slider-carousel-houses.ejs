<script>
    const buttons = document.querySelectorAll("[data-carousel-btn]");
    const carousel = document.querySelector("[data-carousel]");
    const tiles = document.querySelectorAll("[data-tile]");
    const container = document.querySelector("[data-test]");
    var width = (carousel.offsetWidth/5);
    var screenWidth = window.innerWidth;
    var tileCount = Math.round(screenWidth/width);
    var slideOpen = false;
    let middleIdx = 0;
    let Dx = 0;
    let count = 0;
    let autoScrolling;

    if(!autoScrolling)
        startAutoScrolling();
    
    if(tileCount === 3)
        tiles[0].classList.add("middle");
    else {
        let temp = document.querySelector(".middle");
    if(temp)
        temp.classList.remove("middle");
    }  

    function startAutoScrolling() {
        autoScrolling = setInterval(autoScroll, 7000);
    }

    function stopAutoScrolling() {
        autoScrolling = clearInterval(autoScrolling);
    }
    
    buttons.forEach(button => {
        button.addEventListener("mouseenter", () => {
            if(tileCount > 1 && autoScrolling) {
                stopAutoScrolling();
            }
        })
    })

    buttons.forEach(button => {
        button.addEventListener("mouseleave", () => {
            if(tileCount > 1 && !autoScrolling) {
                startAutoScrolling();
            }
        })
    })

    container.addEventListener("mouseenter", () => {
        if(tileCount > 1)
            stopAutoScrolling();
    })

    container.addEventListener("mouseleave", () => {
        if(tileCount > 1 && !slideOpen)
            startAutoScrolling();
    })

    window.addEventListener("resize", () => { 
        screenWidth = window.innerWidth;
        width = (carousel.offsetWidth/5);
        tileCount = Math.round(screenWidth/width)
        count = 0;
        middleIdx = 0;
        carousel.style.transform = "translate(0)";
        if(tileCount > 1 && !autoScrolling)
            startAutoScrolling();
        
        if(tileCount != 3) {
            let temp = document.querySelector(".middle");
            if(temp)
                temp.classList.remove("middle");
        } else {
            let temp = document.querySelector(".middle");
            if(temp)
                temp.classList.remove("middle");
            tiles[0].classList.add("middle");
        }
    });

    buttons.forEach(button => {
        button.addEventListener("click", () => {
            if(button.getAttribute("data-carousel-btn") === "prev")
                scrollLeft();
            else if(button.getAttribute("data-carousel-btn") === "next")
                scrollRight();
        })
    })

    function changeMiddle() {
        let temp = document.querySelector(".middle");
        if(temp)
            temp.classList.remove("middle");
        tiles[middleIdx].classList.add("middle");
    }

    function autoScroll() {
        if(tileCount === 1) {
            autoScrolling = clearInterval(autoScrolling);
        }
        else
            scrollRight();
    }

    function scrollLeft() {
        if(count > 0) {
            if(count === 1) {
                --count;
                Dx = 0;
            }
            else 
            Dx = -(--count * width);
            if(tileCount === 3) {
                middleIdx--;
                changeMiddle();
            }
        } else {
            count = 5 - tileCount;
            Dx = -((5 - tileCount) * width)
            if(tileCount === 3) {
                middleIdx = 2;
                changeMiddle();
            }
        }

        carousel.style.transform = "translate(" + Dx + "px)"; 
    }

    function scrollRight() {
        if(count < (5 - tileCount)) {
            Dx = -(++count * width);
            if(tileCount === 3) {
                middleIdx++;
                changeMiddle();
            }
        } else {
            Dx = 0;
            count = 0;
            if(tileCount === 3) {
                middleIdx = 0;
                changeMiddle();
            }
        }
        
        carousel.style.transform = "translate(" + Dx + "px)";
    }
</script>

